    <script>
        $(document).scroll(function () {
            if ($('.scrollTopValue').length) {
                $('.scrollTopValue').val($(document).scrollTop());
            }
        });
        $(document).ready(function () {
            if ($('.scrollTopValue').length && $('.scrollTopValue').val().length) {
                $(document).scrollTop($('.scrollTopValue').val());
            }
        });
    </script>


